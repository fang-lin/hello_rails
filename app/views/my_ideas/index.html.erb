<p id="notice"><%= notice %></p>

<h1>My Ideas</h1>

<table>
  <thead>
    <tr>
      <th>Picture des</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_ideas.each do |my_idea| %>
      <tr>
        <td><%= my_idea.picture_des %></td>
        <td><%= my_idea.picture %></td>
        <td><%= link_to 'Show', my_idea %></td>
        <td><%= link_to 'Edit', edit_my_idea_path(my_idea) %></td>
        <td><%= link_to 'Destroy', my_idea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My Idea', new_my_idea_path %>
